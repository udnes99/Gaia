{
    "compileOnSave": false,
    "compilerOptions": {
      "rootDir": ".",
      "sourceMap": true,
      "declaration": false,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "importHelpers": true,
      "target": "es2015",
      "module": "commonjs",
      "lib": ["esnext", "DOM"],
      "moduleResolution": "node",
      "skipLibCheck": true,
      "skipDefaultLibCheck": true,
      "esModuleInterop": true,
      "noEmit": true,
      "baseUrl": ".",
      "useUnknownInCatchVariables": false,
      "resolveJsonModule": true
    },
    "exclude": ["node_modules"]
  }